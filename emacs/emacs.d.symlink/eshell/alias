alias d dired $1
alias ff find-file $1
alias clear recenter 0

alias l  ls -1A $*
alias ll ls -lh $*
alias la ll -A $*
alias lc ll -trc $*
alias lk ll -Sr $*
alias lr ll -R $*
alias lt ll -tr $*
;; alias lm la $* | "$PAGER"
alias lu lt -u $*
alias lx lt -XB $*

alias g git $*
alias s g status -sb $*
alias d g diff $*
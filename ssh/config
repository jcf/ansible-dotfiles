host *.amazonaws.com
  User ubuntu
  StrictHostKeyChecking no
  UserKnownHostsFile ~/.ssh/ec2_hosts

# Heroku Accounts

Host heroku.logi.cl
  HostName heroku.com
  IdentityFile ~/.ssh/identity.heroku.logi.cl
  IdentitiesOnly yes

Host heroku.work
  HostName heroku.com
  IdentityFile "/Users/jcf/.ssh/identity.heroku.work"
  IdentitiesOnly yes

# vim: syn=sshconfig

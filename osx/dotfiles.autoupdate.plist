<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>dotfiles.autoupdate</string>
    <key>OnDemand</key>
    <true/>
    <key>RunAtLoad</key>
    <false/>
    <key>UserName</key>
    <string>jcf</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/jcf/.dotfiles/bin/dotfiles</string>
        <string>update</string>
        <string>--skip-repo</string>
        <string>--syslog</string>
    </array>
    <key>StandardOutPath</key>
    <string>/Users/jcf/Library/Logs/dotfiles.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/jcf/Library/Logs/dotfiles.error.log</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>00</integer>
        <key>Minute</key>
        <integer>00</integer>
    </dict>
    <key>WorkingDirectory</key>
    <string>/Users/jcf/.dotfiles</string>
</dict>
</plist>

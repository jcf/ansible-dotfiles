# -*- mode: conf -*-
defaults
host smtp.gmail.com
port 587
protocol smtp
auth on
tls on
tls_trust_file /usr/local/opt/curl-ca-bundle/share/ca-bundle.crt

account gmail
from jamesconroyfinn@gmail.com
user jamesconroyfinn@gmail.com

account listora
from jcf@listora.com
user jcf@listora.com

account logicl
from james@logi.cl
user james@logi.cl

account default : logicl
